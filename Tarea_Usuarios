SELECT * FROM users;


SELECT email
FROM users
WHERE id = 1;

SELECT * FROM users 
WHERE id = 3 ORDER BY id DESC;

UPDATE users 
SET last_name = 'Panqueques'
WHERE id = 3;

DELETE  FROM users 
WHERE id = 2;

SELECT first_name
FROM users
ORDER BY first_name
